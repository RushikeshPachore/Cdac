package interviewpractice;
 import java.util.Scanner;

public class p2 {
	public static void main(String args []) {
		
		int num , factorial=1;
		
		Scanner sc= new Scanner(System.in);
		
		System.out.println("enter number to get its factorial");
		
		num=sc.nextInt();
		for(int i=1 ; i<=num; i++) {
			factorial= factorial*i;
			
		}
	}

}
